{
  "name": "vision",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "kind-of": "^6.0.3",
    "mocha": "^6.2.3",
    "prettier": "^1.19.1",
    "semver": "^6.3.0",
    "set-value": "^3.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "run": "npm run clean && npm run webpack && npm run local",
    "clean": "node -e \"require('./build-script.js').clean()\"",
    "webpack": "webpack --config webpack.config.js",
    "local": "webpack-dev-server --mode development --open --hot",
    "build": "npm run version && npm run lint:prod && npm run test && echo '\n\nBuild OK.'",
    "version": "node -e \"require('./build-script.js').version()\"",
    "lint:prod": "eslint --format stylish ./ src/",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha ./tst/*test.js",
    "test:browser": "karma run",
    "karma": "karma start",
    "lint:dev": "eslint --format codeframe --fix ./ src/"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
